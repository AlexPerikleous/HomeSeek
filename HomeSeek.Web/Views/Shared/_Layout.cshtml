    <script src="~/js/map-plugins.js"></script>
    <script src="~/js/map-listing.js"></script>
    @*<script src="~/js/map-single.js"></script>*@
    <script src="~/js/map-add.js"></script>
    <script src="~/js/pjs.js"></script>
    <script src="~/js/shop.js"></script>

</body>
</html>
